<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.rpm.mapper.add.AddUserMapper">
	<insert id="addUsers" parameterType="User">
		insert into user_t(username,idcard,telephone,occupancy_type,occupancy_number,registration_time,describe,house_name,from_id)
		values(#{username},#{idcard},#{telephone},#{occupancy_type},#{occupancy_number},#{registration_time},#{describe},#{house_name},#{from_id})
	</insert>
</mapper>